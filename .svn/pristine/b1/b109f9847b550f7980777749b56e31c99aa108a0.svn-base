<?xml version='1.0' encoding='utf-8' ?>
<widget id="com.quarks.paipaiwang" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>paimeet</name>
    <description>
        拍米，专业的网购平台。
    </description>
    <author email="974091129@qq.com" href="http://www.quarktimes.com">
        YANG杨
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-windowSoftInputMode" value="stateVisible|adjustResize" />
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.GeoBroker" />
    </feature>
    <platform name="android">
        <icon src="www/ldpi.png" density="ldpi" />
        <icon src="www/mdpi.png" density="mdpi" />
        <icon src="www/hdpi.png" density="hdpi" />
        <icon src="www/xhdpi.png" density="xhdpi" />
    </platform>
    <platform name="ios">
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="www/ios-icon-60.png" width="60" height="60" />
        <icon src="www/ios-icon-60@2x.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="www/ios-icon-76.png" width="76" height="76" />
        <icon src="www/ios-icon-76@2x.png" width="152" height="152" />
        <!-- iOS 6.1 -->
        <!-- Spotlight Icon -->
        <icon src="www/ios-icon-40.png" width="40" height="40" />
        <icon src="www/ios-icon-40@2x.png" width="80" height="80" />
        <!-- iPhone / iPod Touch -->
        <icon src="www/ios-icon.png" width="57" height="57" />
        <icon src="www/ios-icon@2x.png" width="114" height="114" />
        <!-- iPad -->
        <icon src="www/ios-icon-72.png" width="72" height="72" />
        <icon src="www/ios-icon-72@2x.png" width="144" height="144" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="www/ios-icon-small.png" width="29" height="29" />
        <icon src="www/ios-icon-small@2x.png" width="58" height="58" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="www/ios-icon-50.png" width="50" height="50" />
        <icon src="www/ios-icon-50@2x.png" width="100" height="100" />
    </platform>
    <preference name="SplashScreen" value="paimeetscreen.png" />
    <preference name="SplashScreenDelay" value="2000" />
</widget>
