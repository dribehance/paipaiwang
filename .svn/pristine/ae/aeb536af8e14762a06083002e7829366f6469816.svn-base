<?xml version='1.0' encoding='utf-8'?>
<widget id="com.quarks.paipaiwang" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <icon density="ldpi" src="www/icon/ldpi.png" />
    <icon density="mdpi" src="www/icon/mdpi.png" />
    <icon density="hdpi" src="www/icon/hdpi.png" />
    <icon density="xhdpi" src="www/icon/xhdpi.png" />
    <name>paimeet</name>
    <description>
        拍米，专业的网购平台。
    </description>
    <author email="974091129@qq.com" href="http://www.quarktimes.com">
        YANG杨
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-windowSoftInputMode" value="stateVisible|adjustResize" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
