<div class="row" ng-repeat="record in records.list">
	<div class="col-xs-3">
		<div class="box-tb text-red x-small nowrap">
			{{ record.username }}
		</div>
	</div>
	<div class="col-xs-6">
		<div class="box-tb text-center text-grey x-small nowrap">
			颜色：{{ record.meta_1 }}&nbsp;&nbsp;&nbsp;
			尺码：{{ record.meta_2 }}&nbsp;&nbsp;&nbsp;
			数量：{{ record.total }}
		</div>
	</div>
	<div class="col-xs-3">
		<div class="box-tb text-right text-grey x-small nowrap">
			{{ record.post_time }}
		</div>
	</div>
	<div class="line col-xs-12"></div>
</div>