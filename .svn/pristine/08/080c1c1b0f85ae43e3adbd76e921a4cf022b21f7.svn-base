<script type="text/ng-template" id="index-cache.html">
    <div class="header flexbox">
        <div class="logo text-center flexbox vertical">
            <div class="logo-cn large">拍米</div>
            <div class="logo-en">paimeet</div>
        </div>
    </div>
    <div class="flex-item scroll">
        <div class="banner">
            <wrap-owlcarousel class="owl-carousel" data-options="{autoPlay: 5000, stopOnHover: true,  
        slideSpeed : 300, paginationSpeed : 400, singleItem : true}">
                <div>
                    <a ng-href="#/product/{{banners.cover_1_p }}" ng-click="next()" class="block">
                        <img class="block" ng-src="{{ bannerHost + banners.cover_1 }}" />
                    </a>
                </div>
                <div>
                    <a ng-href="#/product/{{banners.cover_2_p }}" ng-click="next()" class="block">
                        <img class="block" ng-src="{{ bannerHost + banners.cover_2 }}" />
                    </a>
                </div>
                <div>
                    <a ng-href="#/product/{{banners.cover_3_p }}" ng-click="next()" class="block">
                        <img class="block" ng-src="{{ bannerHost + banners.cover_3 }}" />
                    </a>
                </div>
                <div>
                    <a ng-href="#/product/{{banners.cover_4_p }}" ng-click="next()" class="block">
                        <img class="block" ng-src="{{ bannerHost + banners.cover_4 }}" />
                    </a>
                </div>
                <div>
                    <a ng-href="#/product/{{banners.cover_5_p }}" ng-click="next()" class="block">
                        <img class="block" ng-src="{{ bannerHost + banners.cover_5 }}" />
                    </a>
                </div>
                <div>
                    <a ng-href="#/product/{{banners.cover_6_p }}" ng-click="next()" class="block">
                        <img class="block" ng-src="{{ bannerHost + banners.cover_6 }}" />
                    </a>
                </div>
                <div>
                    <a ng-href="#/product/{{banners.cover_7_p }}" ng-click="next()" class="block">
                        <img class="block" ng-src="{{ bannerHost + banners.cover_7 }}" />
                    </a>
                </div>
            </wrap-owlcarousel>
        </div>

        <div class="container">
            <ul class="ls row box">
                <li class="col-xs-3">
                    <a ng-click="next()" href="#/strategy" class="block text-center index-icon">
                        <img ng-src="img/book.png" alt="" class="block">
                        <em class="block text-center x-small nowrap">拍币攻略</em>
                    </a>
                </li>
                <li class="col-xs-3">
                    <a ng-click="next()" href="#/hot" class="block text-center index-icon">
                        <img ng-src="img/hot.png" alt="" class="block">
                        <em class="block text-center x-small nowrap">热卖商品</em>
                    </a>
                </li>
                <li class="col-xs-3">
                    <a ng-click="next()" href="#/catalogs" class="block text-center index-icon">
                        <img ng-src="img/grid.png" alt="" class="block">
                        <em class="block text-center x-small nowrap">分类换购</em>
                    </a>
                </li>
                <li class="col-xs-3">
                    <a ng-click="next()" href="#/auctions" class="block text-center index-icon">
                        <img ng-src="img/hammer.png" alt="" class="block">
                        <em class="block text-center x-small nowrap">竞拍</em>
                    </a>
                </li>
                <li class="clearfix box"></li>
                <li class="col-xs-3">
                    <a ng-click="next()" href="#lucky_auctions" class="block text-center index-icon">
                        <img ng-src="img/clover.png" alt="" class="block">
                        <em class="block text-center x-small nowrap">幸运拍</em>
                    </a>
                </li>
                <li class="col-xs-3">
                    <a ng-click="next()" href="#/charge" class="block text-center index-icon">
                        <img ng-src="img/phone.png" alt="" class="block">
                        <em class="block text-center x-small nowrap">话费充值</em>
                    </a>
                </li>
                <li class="col-xs-3">
                    <a ng-click="next()" href="#/index" class="block text-center index-icon">
                        <img ng-src="img/heart.png" alt="" class="block">
                        <em class="block text-center x-small nowrap">拍米公益</em>
                    </a>
                </li>
                <li class="col-xs-3">
                    <a ng-click="next()" href="#/orders" class="block text-center index-icon">
                        <img ng-src="img/order.png" alt="" class="block">
                        <em class="block text-center x-small nowrap">全部订单</em>
                    </a>
                </li>
            </ul>
        </div>
        <div class="box-mb box-mt">
            <a ng-href="#/product/{{banners.pinpai}}" class="block">
                <img ng-src="img/brand_street.png" alt="" class="block">
            </a>
        </div>

    </div>
    <div class="footer flexbox">
        <div class="flex-item">
            <a href="#/index" ng-click="next()" class="footer-nav footer-icon home-page {{'active-'+ (active=='index')}}">
                <img ng-src="img/home_{{ active=='index' ? 'active':'normal'}}.png" alt="" class="block">
                <span class="xs-small block text-center nowrap">首页</span>
            </a>
        </div>

        <div class="flex-item">
            <a href="#/nearby" ng-click="next()" class="footer-nav footer-icon nearby-page {{'active-'+ (active=='nearby')}}">
                <img ng-src="img/nearby_{{ active=='nearby' ? 'active':'normal'}}.png" alt="" class="block">
                <span class="xs-small block text-center nowrap">同城</span>
            </a>
        </div>

        <div class="flex-item">
            <a href="#/shopping_cart" ng-click="next()" class="footer-nav footer-icon shopping-cart-page {{'active-'+ (active=='shopping_cart')}}">
                <img ng-src="img/shopping_cart_{{ active=='shopping_cart' ? 'active':'normal'}}.png" alt="" class="block">
                <span class="xs-small block text-center nowrap">购物车</span>
            </a>
        </div>

        <div class="flex-item">
            <a href="#/user" ng-click="next()" class="footer-nav footer-icon user-page {{'active-'+ (active=='user')}}">
                <img ng-src="img/user_{{ active=='user' ? 'active':'normal'}}.png" alt="" class="block">
                <span class="xs-small block text-center nowrap">我</span>
            </a>
        </div>
    </div>
</script>
<script type="text/ng-template" id="nearby-cache.html">
    <div class="header flexbox">
        <a href="#/provinces" ng-click="next()" class="cog">{{ city ? city.split("市")[0]:'深圳' }}[切换]</a>
        <div class="flex-item text-center big ng-title">{{ pageName }}</div>
        <div class="cog"></div>
    </div>
    <div class="flex-item scroll">
        <div class="newaddress">
            <a href="#/nearby_stores" ng-click="next()" class="city-4 block">
                <img class="ui-li" ng-src="img/city-home.png">
                <span class="city-3">附近加盟店</span>
            </a>
            <a href="#/nearby_exchanges" ng-click="next()" class="city-4 block">
