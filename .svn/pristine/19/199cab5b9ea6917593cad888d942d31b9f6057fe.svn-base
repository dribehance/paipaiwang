html,body {
	height: 100%;
	width: 100%;
	margin: 0;
	padding: 0;
	font-family: "微软雅黑";
	font-size: 14px;
	background-color: #f4f4f4;
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0); 
	-webkit-tap-highlight-color: transparent;
	-webkit-user-select: none;
	-webkit-touch-callout: none;
}
input {
	-webkit-user-select: auto;
}
.top-page {
	position: relative;
	height: 100%;
	width: 100%;
	overflow: hidden;
}
#page {
	width: 100%;
	height: 100%;
	overflow: hidden;
	background-color: #f4f4f4;
}
.content {
	
}

a{
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0); 
	-webkit-tap-highlight-color: transparent;
	-webkit-user-select: none;
	-webkit-touch-callout: none;
}
a:focus {
	outline: 0;
};